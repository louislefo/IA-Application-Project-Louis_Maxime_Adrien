{
  "project": {
    "name": "AI Football Performance Analyzer",
    "version": "1.0.0",
    "description": "Web application for analyzing and predicting football player performance using XGBoost and Streamlit",
    "author": "Adrien Schuttig, Maxime Laurent, Louis Le Forestier",
    "organization": "ECE Paris / ESILV Nantes",
    "date_created": "2025-12-04"
  },
  
  "quick_start": {
    "windows_batch": "run_app.bat",
    "windows_powershell": "./run_app.ps1",
    "manual": "streamlit run src/application.py",
    "first_time": "cd src && python ml_analysis.py"
  },
  
  "documentation": {
    "index": "INDEX.md",
    "quick_start": "QUICKSTART.md",
    "usage": "USAGE.md",
    "app_interface": "APP_INTERFACE.md",
    "project_summary": "PROJECT_SUMMARY.md",
    "readme": "README.md",
    "cheatsheet": "CHEATSHEET.md",
    "this_file": "project_config.json"
  },
  
  "models": {
    "regression": {
      "name": "XGBoost Regressor",
      "purpose": "Predict overall_rating (0-100)",
      "performance": {
        "r2_score": 0.795,
        "rmse": 3.18,
        "mae": 2.45
      },
      "top_features": [
        {"name": "short_passing", "importance": 0.437},
        {"name": "age", "importance": 0.219},
        {"name": "dribbling", "importance": 0.098}
      ],
      "file": "models/regression_model.pkl"
    },
    
    "classification": {
      "name": "Logistic Regression Classifier",
      "purpose": "Predict career trajectory",
      "performance": {
        "accuracy": 0.89,
        "precision": 0.87,
        "recall": 0.87,
        "f1_score": 0.87
      },
      "classes": [
        "high_growth",
        "likely_improve",
        "stable",
        "decline"
      ],
      "file": "models/classification_model.pkl"
    }
  },
  
  "data": {
    "source_dataset": {
      "name": "FIFA Players Dataset",
      "url": "https://www.kaggle.com/datasets/maso0dahmed/football-players-data",
      "rows": 17954,
      "columns": 51,
      "file": "data/fifa_players.csv"
    },
    
    "features_used": [
      "age",
      "height_cm",
      "weight_kgs",
      "finishing",
      "dribbling",
      "short_passing",
      "acceleration",
      "sprint_speed",
      "stamina",
      "strength"
    ],
    
    "targets": [
      "overall_rating",
      "potential"
    ],
    
    "added_players_database": "data/added_players.csv"
  },
  
  "application": {
    "framework": "Streamlit",
    "port": 8501,
    "main_file": "src/application.py",
    
    "features": [
      "Interactive player input form (sidebar)",
      "Real-time XGBoost predictions",
      "Career trajectory classification",
      "Visual attribute charts",
      "Player database management",
      "Probability visualization",
      "CSV export of results"
    ],
    
    "ui_sections": [
      "Player Information Form (Sidebar)",
      "Player Profile Display",
      "Attribute Visualization",
      "Analysis Results",
      "Career Prediction",
      "Probability Breakdown",
      "Database Viewer",
      "Global Statistics"
    ]
  },
  
  "dependencies": {
    "python_version": "3.10+",
    "packages": [
      {
        "name": "streamlit",
        "version": ">=1.28.0",
        "purpose": "Web framework"
      },
      {
        "name": "xgboost",
        "version": ">=2.0.0",
        "purpose": "XGBoost regression model"
      },
      {
        "name": "scikit-learn",
        "version": ">=1.7.2",
        "purpose": "ML preprocessing & classification"
      },
      {
        "name": "pandas",
        "version": ">=2.3.3",
        "purpose": "Data manipulation"
      },
      {
        "name": "numpy",
        "version": ">=2.3.5",
        "purpose": "Numerical operations"
      },
      {
        "name": "matplotlib",
        "version": ">=3.10.7",
        "purpose": "Visualization"
      },
      {
        "name": "seaborn",
        "version": ">=0.13.2",
        "purpose": "Advanced visualization"
      },
      {
        "name": "joblib",
        "version": ">=1.5.2",
        "purpose": "Model serialization"
      }
    ]
  },
  
  "file_structure": {
    "root": {
      "docs": [
        "README.md",
        "QUICKSTART.md",
        "USAGE.md",
        "APP_INTERFACE.md",
        "PROJECT_SUMMARY.md",
        "CHEATSHEET.md",
        "INDEX.md"
      ],
      "scripts": [
        "run_app.bat",
        "run_app.ps1",
        "test_setup.py",
        "requirements.txt",
        "project_config.json"
      ]
    },
    
    "src": [
      "application.py",
      "ml_analysis.py",
      "ml_analysis.ipynb",
      "ml_advanced_models.ipynb",
      "data_analysis.py",
      "analyse.ipynb"
    ],
    
    "data": [
      "fifa_players.csv (17,954 players)",
      "added_players.csv (user-added)"
    ],
    
    "models": [
      "regression_model.pkl",
      "classification_model.pkl"
    ],
    
    "doc": [
      "Projet.md",
      "code_explanation.md"
    ],
    
    "images": [
      "analyse/ (analysis charts)",
      "ml/ (ML visualizations)"
    ]
  },
  
  "workflow": {
    "step_1_install": {
      "command": "pip install -r requirements.txt",
      "time": "2-3 minutes",
      "description": "Install all Python dependencies"
    },
    
    "step_2_train": {
      "command": "cd src && python ml_analysis.py",
      "time": "2-3 minutes",
      "description": "Train XGBoost and Logistic Regression models",
      "creates": [
        "models/regression_model.pkl",
        "models/classification_model.pkl"
      ]
    },
    
    "step_3_run": {
      "command": "streamlit run src/application.py",
      "time": "1 second",
      "description": "Start the web application"
    },
    
    "step_4_use": {
      "description": "Enter player data and analyze"
    }
  },
  
  "performance_metrics": {
    "regression": {
      "R¬≤ Score": 0.795,
      "RMSE": 3.18,
      "MAE": 2.45,
      "Interpretation": "Model explains 79.5% of rating variance"
    },
    
    "classification": {
      "Accuracy": 0.89,
      "Precision (macro)": 0.87,
      "Recall (macro)": 0.87,
      "F1-Score (macro)": 0.87,
      "Interpretation": "89% of career predictions are correct"
    }
  },
  
  "attribute_ranges": {
    "age": {"min": 15, "max": 40, "typical": "20-35"},
