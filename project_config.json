  "troubleshooting": {
    "models_not_found": {
      "error": "FileNotFoundError: Models not found",
      "solution": "cd src && python ml_analysis.py"
    },
    
    "port_in_use": {
      "error": "Port 8501 already in use",
      "solution": "streamlit run src/application.py --server.port 8502"
    },
    
    "module_not_found": {
      "error": "ModuleNotFoundError: No module named 'xgboost'",
      "solution": "pip install -r requirements.txt"
    },
    
    "slow_startup": {
      "error": "App takes long time to start",
      "solution": "First run loads models to cache. Subsequent runs are faster."
    }
  },
  
  "resources": {
    "official_docs": [
      "https://docs.streamlit.io/",
      "https://xgboost.readthedocs.io/",
      "https://scikit-learn.org/"
    ],
    
    "datasets": [
      "https://www.kaggle.com/datasets/maso0dahmed/football-players-data"
    ],
    
    "tutorials": [
      "Streamlit tutorial",
      "XGBoost guide",
      "Scikit-learn user guide"
    ]
  },
  
  "status": {
